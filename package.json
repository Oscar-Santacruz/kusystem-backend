{
  "name": "fastify-prisma-starter",
  "version": "1.0.0",
  "description": "Production-ready starter for Fastify, Prisma, Zod, and TypeScript.",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "lint": "eslint . --ext .ts --fix",
    "test": "vitest run",
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:seed": "prisma db seed",
    "openapi:generate": "tsx src/docs/openapi.ts"
  },
  "keywords": [
    "fastify",
    "prisma",
    "zod",
    "typescript",
    "starter"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.16.2",
    "@sentry/nestjs": "^10.10.0",
    "@sentry/node": "^7.119.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "newrelic": "^11.15.0",
    "resend": "^6.0.2",
    "swagger-ui-express": "^5.0.1",
    "zod": "^3.23.8",
    "prisma": "^5.16.2"
  },
  "devDependencies": {
    "@asteasolutions/zod-to-openapi": "^7.3.4",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.12",
    "@types/swagger-ui-express": "^4.1.8",
    "tsx": "^4.19.0",
    "typescript": "^5.8.3",
    "vitest": "^1.5.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
